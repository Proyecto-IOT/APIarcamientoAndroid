<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="160dp"
    android:background="@color/design_default_color_primary"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginBottom="15dp">

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Carro"
    android:textSize="25sp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    android:id="@+id/autocompleto"
    android:textColor="@color/md_theme_light_onPrimary"
    />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="color"
        android:textSize="25sp"
        android:id="@+id/colorcoche"
        app:layout_constraintTop_toBottomOf="@+id/autocompleto"
        app:layout_constraintBottom_toTopOf="@+id/lugar_de_estancia"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textColor="@color/md_theme_light_onPrimary"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="lugar"
        android:id="@+id/lugar_de_estancia"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
     app:layout_constraintBottom_toBottomOf="parent"
        android:textColor="@color/md_theme_light_onPrimary"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="placas"
        android:id="@+id/placasdelauto"
        android:textSize="25sp"
        android:layout_marginLeft="30dp"
        app:layout_constraintBottom_toTopOf="@+id/lugar_de_estancia"
        app:layout_constraintTop_toBottomOf="@+id/autocompleto"
        app:layout_constraintLeft_toRightOf="@+id/colorcoche"
        android:textColor="@color/md_theme_light_onPrimary"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="entrada"
        android:id="@+id/fech_entrada"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/placasdelauto"
        app:layout_constraintBottom_toTopOf="@+id/fech_salida"
        android:textColor="@color/md_theme_light_onPrimary"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="salida"
        android:id="@+id/fech_salida"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fech_entrada"
        app:layout_constraintBottom_toBottomOf="parent"
        android:textColor="@color/md_theme_light_onPrimary"
        />





</androidx.constraintlayout.widget.ConstraintLayout>